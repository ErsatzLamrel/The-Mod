package nat.nshm.blocks;

import nat.nshm.Reference;
import net.minecraft.block.Block;
import net.minecraft.block.material.Material;

public class BlockDwarfStar extends Block {

	public BlockDwarfStar() {
		super(Material.ROCK);
		setUnlocalizedName(Reference.SuperHeroBlocks.DWARFSTARBLOCK.getUnlocalizedName());
		setRegistryName(Reference.SuperHeroBlocks.DWARFSTARBLOCK.getRegistryName());
		setHardness(1.0F);
		
	}
}
